<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.app7.dao.ElectricMeterDao" >
  <update id="charge">
    update electric_meter set electricity = electricity+#{delta} where eid = #{eid}
  </update>
</mapper>